{
  "name": "svelte-theme-components",
  "version": "1.0.0",
  "license": "OBELISK",
  "author": {
    "name": "Obelisk Services",
    "email": "info@obelisk-services.com",
    "url": "https://obelisk-services.com"
  },
  "homepage": "https://github.com/obelisk-services/svelte-theme-components",
  "bugs": {
    "url": "https://github.com/obelisk-services/svelte-theme-components/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "@sveltejs/kit": "^2.0.0",
    "svelte": "^5.0.0 || ^4.0.0"
  },
  "devDependencies": {
    "@sveltejs/kit": "^2.5.27",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.45.1",
    "globals": "^15.0.0",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.2.6",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tslib": "^2.4.1",
    "typescript": "^5.5.0",
    "vite": "^5.0.0"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "dependencies": {
    "nprogress": "^0.2.0",
    "svelte-js-functions-package": "git+https://git@github.com/obelisk-services/svelte-js-functions-package.git"
  },
  "keywords": [
    "Svelte",
    "SvelteKit",
    "SvelteKit Js functions"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint ."
  }
}
